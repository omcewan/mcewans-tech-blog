<ol>
  <li>
    {{#each posts}}
      <div class='card'>
        <div class='card-header'>
          <p class='title'>Title: {{title}}</p>
        </div>
        <div class='card-body'>
          <p class='posted-by'>Posted By: {{user.username}} on {{createdAt}}</p>
          <p class='card-text'>{{post_contents}}</p>
        </div>
      </div>
    {{/each}}
  </li>
</ol>